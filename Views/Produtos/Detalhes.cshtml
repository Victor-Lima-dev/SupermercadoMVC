@model supermercadoMVC.Models.Produto


<div class="container-fluid">
	<div class="row">
	<div class="col-md-4">
			<div class="card">
				<h5 class="card-header">
					@Model.Nome
				</h5>
				<div class="card-body">
					<p class="card-text">
						@Model.Descricao
					</p>
					<p class="card-text">
						@Model.Preco
					</p>
				</div>